{
  "compilerOptions": {
    "target": "es6",
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "src",
    "paths": {
      "Breadcrumb/*": [
        "Breadcrumb/*"
      ],
      "Components/*": [
        "Components/*"
      ],
      "Container/*": [
        "Container/*"
      ],
      "Context/*": [
        "Context/*"
      ],
      "EnhancedForm/*": [
        "EnhancedForm/*"
      ],
      "InfoPage/*": [
        "InfoPage/*"
      ],
      "lib/*": [
        "lib/*"
      ],
      "mock/*": [
        "mock/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "src/serviceWorker.js"
  ]
}